<!DOCTYPE html>
<html lang="ru">


<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.0/css/font-awesome.min.css">



</head>
<body>
    <header>

      
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <style type="text/css">
        @@font-face {
            font-family: "a_FuturaRound";
            src: url(../../Fonts/FuturaRoundBold.eot);
            src: url(../../Fonts/FuturaRoundBold.eot?#iefix), url(../../Fonts/FuturaRoundBold.woff) format('woff'), url('/Fonts/FuturaRoundBold.ttf') format("truetype");
            font-weight: normal;
            font-style: normal;
        }

        body {
            font-family: a_FuturaRound,Arial;
            background: #efefef
        }

        nav {
            font-weight: bold
        }
    </style>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
